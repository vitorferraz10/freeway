        "maxWidth": "26.48px",
        "maxHeight": "19.76px",
        "blockClass": "img-payment-mobile-submenu"
    }
}, 
"image#payment-4": {
    "props": {
        "src": "assets/payment-menu-4.png",
        "maxWidth": "34.2px",
        "maxHeight": "14.27px",
        "blockClass": "img-payment-mobile-submenu"
    }
}, 
"image#payment-5": {
    "props": {
        "src": "assets/payment-menu-5.png",
        "maxWidth": "26.48px",
        "maxHeight": "13.18px",
        "blockClass": "img-payment-mobile-submenu"
    }
}, 
"image#payment-6": {
    "props": {
        "src": "assets/payment-menu-6.png",
        "maxWidth": "22.06px",
        "maxHeight": "17.57px",
        "blockClass": "img-payment-mobile-submenu"
    }
}, 
"image#payment-7": {
    "props": {
        "src": "assets/payment-menu-7.png",
