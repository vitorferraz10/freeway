{
  "drawer-trigger#ajuda": {
    "children": ["rich-text#menu-ajuda"],
    "props": {
      "blockClass": "drawer-trigger"
    }
  },
  "rich-text#menu-ajuda": {
    "props": {
      "text": "Ajuda",
      "blockClass": "menu-ajuda"
    }
  },
  "drawer#ajuda": {
    "children": ["flex-layout.row#drawer-ajuda-menu"],
    "blocks": ["drawer-header#ajuda", "drawer-trigger#ajuda"],
    "props": {
      "slideDirection": "rightToLeft",
      "blockClass": "drawe-ajuda-main"
    }
  },

  "flex-layout.row#drawer-ajuda-menu": {
    "children": ["flex-layout.col#ajuda-drawwer"],
    "props": {
      "blockClass": "drawer-ajuda-col"
    }
  },
  "flex-layout.col#ajuda-drawwer": {
    "children": [
      "menu-item#atendimento",
      "menu-item#institucional",
      "flex-layout.row#payment-sub-menu-ajuda", 
      "flex-layout.row#sociais-sub-menu-ajuda"
    ]
  },
  "flex-layout.row#payment-sub-menu-ajuda": {
    "children": ["rich-text#payment-metods-title", "flex-layout.col#payment-img-menu-ajuda"], 
    "props": {
        "blockClass": "payment-sub-menu-ajuda", 
        "preventHorizontalStretch": true
    }
  },

"flex-layout.col#payment-img-menu-ajuda": {
"children": ["flex-layout.row#col-1-image-payment", "flex-layout.row#col-2-image-payment"], 
"props": {
    "blockClass": "col-image-payment", 
    "preventHorizontalStretch": true
}
  },
  "flex-layout.row#col-1-image-payment": {
      "children": ["image#payment-1", "image#payment-2", "image#payment-3", "image#payment-4"], 
      "props": {
          "blockClass": "image-payment-menu"
      }
  },
  "flex-layout.row#col-2-image-payment": {
    "children": ["image#payment-5", "image#payment-6", "image#payment-7", "image#payment-8"],
    "props": {
        "blockClass": "image-payment-menu"
    }
  },

  "image#payment-1": {
      "props": {
        "src": "assets/payment-menu-1.png",
        "maxWidth": "24.27px",
        "maxHeight": "15.37px",
      "blockClass": "img-payment-mobile-submenu"
      }
  }, 
  "image#payment-2": {
    "props": {
        "src": "assets/payment-menu-2.png",
        "maxWidth": "30px",
        "maxHeight": "9.88px",
        "blockClass": "img-payment-mobile-submenu"
    }
}, 
"image#payment-3": {
